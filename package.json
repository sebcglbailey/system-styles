{
  "name": "cs-layou",
  "description": "Layout your artboards based on margins provided in the layer names. The plugin also offsets for capheight and baseline discrepencies.",
  "version": "1.0.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "cs-layout",
    "manifest": "src/manifest.json",
    "main": "cs-layout.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "Seb Bailey <sebastian@clearscore.com>",
  "repository": "https://github.com/sebcglbailey/cs-layout",
  "dependencies": {
    "skpm": "^1.1.11"
  }
}
