{
  "name": "system-styles",
  "description": "Generate styles for your design system based off a single list of text layers, and colour fills.",
  "version": "2.1.1",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "system-styles",
    "manifest": "src/manifest.json",
    "main": "system-styles.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "Seb Bailey <sebastian@clearscore.com>",
  "repository": "https://github.com/sebcglbailey/system-styles",
  "dependencies": {
    "skpm": "^1.1.11"
  }
}
